{
    "silt": {
        "include": [
            {
                "dest": "child::store",
                "file": "exp_common.json",
                "src": "child::store0"
            },
            {
                "dest": "child::store",
                "file": "exp_common.json",
                "src": "child::store1"
            },
            {
                "dest": "child::store",
                "file": "exp_common.json",
                "src": "child::store2"
            }
        ],
        "store": {
            "data-len": 1000,
            "id": {},
            "key-len": 20,
            "stage-limit": 2,
            "store1-high-watermark": 1,
            "store1-low-watermark": 0,
            "temp-file": "./silt.db/tmp/",
            "type": "combi"
        }
    }
}
